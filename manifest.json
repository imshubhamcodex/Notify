{
    "name": "Notify",
    "version": "0.0.0",
    "manifest_version": 2,
    "content_scripts" : [
		{	
			"matches" : [
				"https://www.tradingview.com/*"
			],
			"js" : ["main.js"]
		}
	],
    "background" : {
		"scripts": ["background.js"]
	},
    "permissions": [
      "notifications"
    ],
    "browser_action" : {
		"default_icon" : "pp.png"
	}
}